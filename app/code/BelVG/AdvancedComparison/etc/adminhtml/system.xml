<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="belvg" translate="label" sortOrder="200000">
            <label><![CDATA[<div style="background:url(data:image/png;base64,) no-repeat; width: 155px; height: 20px;"></div>]]></label>
        </tab>
        <section id="advancedcomparison" translate="label" sortOrder="140" showInDefault="1" showInWebsite="1" showInStore="1">
            <class>separator-top</class>
            <label>Advanced Comparison</label>
            <tab>belvg</tab>
            <resource>BelVG_AdvancedComparison::config_advancedcomparison</resource>
            <group id="settings" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                <label>Settings</label>
                <hide_in_single_store_mode>0</hide_in_single_store_mode>
                <comment>
                    <![CDATA[
                    <div style="padding:10px;background-color:#fff;border:1px solid #ddd;margin-bottom:7px;">
                    <div style="float:left;margin-right:10px;background:url('data:image/png;base64,') no-repeat; width: 54px; height: 54px;"></div>
                    This module was developed by <a href="http://www.belvg.com" target="_blank">www.BelVG.com</a>.<br>
                    For docs & examples refer to the BelVG <a href="http://www.belvg.com" target="_blank">web site</a>.
                    <br/>We have the largest selections of  <a href="http://store.belvg.com/extensions.html" target="_blank">extensions</a> and <a href="http://store.belvg.com/themes.html" target="_blank">themes</a> on Magento.
                    </div>
                    ]]>
                </comment>
                <field id="enabled" translate="label" type="select" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="enabled_rating" translate="label" type="select" sortOrder="20" showInDefault="1" showInWebsite="1" showInStore="1">
                    <label>Enabled rating</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>
        </section>
    </system>
</config>